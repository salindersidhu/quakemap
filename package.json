    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "gh-pages": "^3.2.3",
    "globby": "^11.0.1",
    "postcss": "^8.4.12",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.0.24"
  }
